{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-opentelemetry": "*",
        "ext-pdo_pgsql": "*",
        "ext-redis": "*",
        "api-platform/core": "^4.2.13",
        "beberlei/doctrineextensions": "^1.5",
        "doctrine/doctrine-bundle": "^3.2.2",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.6.1",
        "easycorp/easyadmin-bundle": "^4.27.7",
        "flow-php/etl": "^0.30.0",
        "flow-php/etl-adapter-parquet": "^0.30.0",
        "flow-php/filesystem-async-aws-bridge": "^0.30.0",
        "flow-php/parquet": "^0.30.0",
        "nyholm/psr7": "^1.8.2",
        "open-telemetry/api": "^1.7.1",
        "open-telemetry/exporter-otlp": "^1.3.4",
        "open-telemetry/gen-otlp-protobuf": "^1.8",
        "open-telemetry/opentelemetry-auto-doctrine": "^0.3.1",
        "open-telemetry/opentelemetry-auto-pdo": "^0.3.0",
        "open-telemetry/opentelemetry-auto-symfony": "^1.1",
        "open-telemetry/opentelemetry-sqlcommenter": "^0.1.0",
        "open-telemetry/sdk": "^1.11",
        "phpdocumentor/reflection-docblock": "^6.0",
        "phpstan/phpdoc-parser": "^2.3.1",
        "symfony/apache-pack": ">=1.0.1",
        "symfony/asset": "8.0.*",
        "symfony/asset-mapper": "8.0.*",
        "symfony/console": "8.0.*",
        "symfony/doctrine-messenger": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/expression-language": "8.0.*",
        "symfony/flex": "^2.10",
        "symfony/form": "8.0.*",
        "symfony/framework-bundle": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/intl": "8.0.*",
        "symfony/lock": "8.0.*",
        "symfony/mailer": "8.0.*",
        "symfony/mime": "8.0.*",
        "symfony/monolog-bundle": "^3.11.1|^4.0.1",
        "symfony/notifier": "8.0.*",
        "symfony/process": "8.0.*",
        "symfony/property-access": "8.0.*",
        "symfony/property-info": "8.0.*",
        "symfony/rate-limiter": "8.0.*",
        "symfony/runtime": "8.0.*",
        "symfony/scheduler": "8.0.*",
        "symfony/security-bundle": "8.0.*",
        "symfony/serializer": "8.0.*",
        "symfony/stimulus-bundle": "^2.32",
        "symfony/string": "8.0.*",
        "symfony/translation": "8.0.*",
        "symfony/twig-bundle": "8.0.*",
        "symfony/uid": "8.0.*",
        "symfony/ux-chartjs": "^2.32",
        "symfony/ux-live-component": "^2.32",
        "symfony/ux-turbo": "^2.32",
        "symfony/validator": "8.0.*",
        "symfony/web-link": "8.0.*",
        "symfony/yaml": "8.0.*",
        "twig/extra-bundle": "^2.12|^3.22.2",
        "twig/string-extra": "^3.22.1",
        "twig/twig": "^2.12|^3.22.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "tbachert/spi": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/Telemetry/_register.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92.5",
        "mtdowling/jmespath.php": "^2.8",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.33",
        "phpstan/phpstan-doctrine": "^2.0.12",
        "phpstan/phpstan-symfony": "^2.0.9",
        "phpunit/phpunit": "^12.5.6",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*",
        "symfony/debug-bundle": "8.0.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*",
        "zenstruck/browser": "^1.10",
        "zenstruck/messenger-test": "^1.13"
    }
}
